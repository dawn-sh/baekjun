package baekjoon;

import java.util.Scanner;

public class B2525 {

	public static void main(String[] args){
		// TODO Auto-generated method stub
		
		Scanner sc=new Scanner(System.in);
		int h=sc.nextInt();
		int m=sc.nextInt();
		int set_h=sc.nextInt();
		
		if(set_h+m<60)
			System.out.println(h+" "+(set_h+m));
		else if(set_h+m>=120)
			System.out.println(h+2+" "+(60-set_h-m));
	}

}
