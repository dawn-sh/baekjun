package baekjoon;

import java.util.Scanner;

public class B15649 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner sc=new Scanner(System.in);
		
		int n=sc.nextInt();
		int m=sc.nextInt();
		
		for(int i=1;i<=n;i++)
		{
			System.out.println(i+" ");
			for(int j=1;j<=m;j++)
			{
				System.out.println(i);
			}
		}
	}

}
