package baekjoon;

import java.io.IOException;
import java.math.BigInteger;
import java.util.Scanner;

public class B1676 {

	//BigInteger는 무한대 정수 표현가능
	public static BigInteger factorial(int n)
	{
		if(n==0 || n==1)
		{
			BigInteger result=new BigInteger("1");
			return result.add(BigInteger.ONE);
		}
		else
		{
			BigInteger fac=BigInteger.valueOf(n);
			return fac.multiply(factorial(n-1));
		}
	}
	
	public static void main(String[] args) throws NumberFormatException, IOException {
		// TODO Auto-generated method stub

		Scanner sc=new Scanner(System.in);
		
		long num=sc.nextInt();
		
		int zeroCnt=0;
		
		String zero=factorial(num).toString();
		
		System.out.println(zero);
		int zeroArr[]=new int[zero.length()];
		
		for(int i=zero.length()-1;i>=0;i--)
		{
			zeroArr[i]=zero.charAt(i)-'0';
		}
		
		for(int i=zeroArr.length-1;i>=0;i--)
		{
			if(zeroArr[i]==0)
				zeroCnt++;
			else
				break;
		}
		
		System.out.println(zeroCnt);
	}

}
