package baekjoon;

import java.util.Scanner;

public class B2438 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner sc=new Scanner(System.in);
		
		int N=sc.nextInt();
		
		for(int i=0;i<N;i++)
		{
			System.out.println("*");
			if(i==1)
				System.out.println("*");
		}
	}

}
