package baekjoon;

import java.util.Scanner;

public class B2745 {
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner sc=new Scanner(System.in);
		
		String N = sc.nextLine();
		int B = sc.nextInt();
		
		int il=(Integer.parseInt(N)%B)*B-1;
		
		System.out.println(il);
		
	}
}
