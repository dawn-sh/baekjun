package baekjoon;

import java.util.Scanner;

public class B10950 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner sc=new Scanner(System.in);
		
		int t=sc.nextInt();
		int a=sc.nextInt();
		int b=sc.nextInt();
		
		System.out.println(t);
		for(int i=0;i<t;i++)
		{
			System.out.println(a+b);
		}
			
	}

}
